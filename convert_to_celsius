#include<stdio.h>
int converttocelsius(float fahrenheit);
void main(){
   float fahrenheit,celsius;
   //prompt user to enter fahrenheit
   printf("Enter fahrenheit:");
   scanf("%f",&fahrenheit);

   //function calling
   celsius=converttocelsius(fahrenheit);

   //print the celsius
   printf("The temperature in celsius is: %.2f\n",celsius);
}
int converttocelsius(float fahrenheit){
    float celsius;
    celsius=(fahrenheit-32)*5/9;
    return celsius;
}
